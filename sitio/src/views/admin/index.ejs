<body class="index">
    <table>
        <thead>
        <tr>
            <th>Imagen</th>
            <th>Nombre</th>
            <th>Precio</th>
            <th>Categoria</th>
            <th>Oferta</th>
        </tr>
        </thead>
        <tbody>
            <% products.forEach(product => { %>
              <tr>
            <td><%= product.img %> </td>
            <td><%= product.name %> </td>
            <td><%= product.price %> </td>
            <td><%= product.category %> </td>
            <td><%= product.sale %> </td>
        </tr>
            <% }) %>
       
        <!-- <tr>
